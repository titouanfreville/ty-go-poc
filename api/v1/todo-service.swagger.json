  "paths": {},
  "definitions": {
    "v1CreateResponse": {
      "type": "object",
      "properties": {
        "api": {
          "type": "string"
        },
        "id": {
          "type": "string",
          "format": "int64",
          "title": "ID of created task"
        }
      },
      "title": "Response that contains data for created todo task"
    },
    "v1ReadAllResponse": {
      "type": "object",
      "properties": {
        "api": {
          "type": "string",
          "title": "API versioning: it is my best practice to specify version explicitly"
        },
        "toDos": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/v1ToDo"
          },
          "title": "List of all todo tasks"
        }
      },
      "title": "Contains list of all todo tasks"
    },
    "v1ReadResponse": {
      "type": "object",
      "properties": {
        "api": {
          "type": "string",
          "title": "API versioning: it is my best practice to specify version explicitly"
        },
        "toDo": {
          "$ref": "#/definitions/v1ToDo",
          "title": "Task entity read by ID"
        }
      },
      "title": "Contains todo task data specified in by ID request"
    },
    "v1ToDo": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "int64"
        },
        "resume": {
          "type": "string"
        },
        "content": {
          "type": "string"
        },
        "reporter_id": {
          "type": "string",
          "format": "int64"
        },
        "worker_id": {
          "type": "string",
          "format": "int64"
        },
        "status": {
          "type": "string"
        }
      },
      "title": "Task we have to do"
    },
    "v1UpdateResponse": {
      "type": "object",
      "properties": {
        "api": {
          "type": "string",
          "title": "API versioning: it is my best practice to specify version explicitly"
        },
        "updated": {
          "type": "string",
          "format": "int64",
          "title": "Contains number of entities have beed updated\nEquals 1 in case of succesfull update"
        }
      },
      "title": "Contains status of update operation"
    }
  }
